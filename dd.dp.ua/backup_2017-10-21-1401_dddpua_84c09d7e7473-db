# End of data contents of table `wp_wpsc_download_status`

# Table: `wp_wpsc_meta`
# Approximate rows expected in table: 6

# Delete any existing table `wp_wpsc_meta`

DROP TABLE IF EXISTS `wp_wpsc_meta`;

# Table structure of table `wp_wpsc_meta`

CREATE TABLE `wp_wpsc_meta` (
  `meta_id` bigint(20) NOT NULL AUTO_INCREMENT,
  `object_type` varchar(24) NOT NULL DEFAULT 'cart_Item',
  `object_id` bigint(20) NOT NULL DEFAULT '0',
  `meta_key` varchar(255) DEFAULT NULL,
  `meta_value` longtext,
  PRIMARY KEY (`meta_id`),
  KEY `object_type__meta_key` (`object_type`,`meta_key`),
  KEY `object_type__object_id__meta_key` (`object_type`,`object_id`,`meta_key`)
) ENGINE=MyISAM AUTO_INCREMENT=7 DEFAULT CHARSET=utf8 ;

