  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`),
  KEY `last_active` (`last_active`),
  KEY `expires` (`expires`),
  KEY `created` (`created`)
) ENGINE=MyISAM AUTO_INCREMENT=5 DEFAULT CHARSET=utf8 ;

